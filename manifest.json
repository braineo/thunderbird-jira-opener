{
  "manifest_version": 2,
  "name": "JIRA opener",
  "description": "Search project key metioned in email and open JIRA page ",
  "version": "1.0",
  "author": "Binbin Ye",
  "applications": {
    "gecko": {
      "id": "binbin.ye@mujin.co.jp",
      "strict_min_version": "66.0"
    }
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Open JIRA",
    "default_icon": "images/internet-32px.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "activeTab",
    "messagesRead",
    "storage"
  ],
  "icons": {
    "64": "images/internet.png",
    "32": "images/internet-32px.png",
    "16": "images/internet-16px.png"
  }
}
